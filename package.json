{
  "name": "react-templater",
  "version": "1.0.0",
  "description": "这是一个基础的基于 webpack + typescript + react 的项目模板。",
  "main": "./src/app.tsx",
  "scripts": {
    "build": "npm run lint && cross-env NODE_ENV=production webpack --progress --colors --display-error-details",
    "start": "npm run lint && cross-env NODE_ENV=development webpack-dev-server --inline --hot --colors",
    "lint": "tslint --exclude src/**/__test__/*.{ts,tsx} ./src/**/*.{ts,tsx}"
  },
  "keywords": [
    "webpack",
    "typescript",
    "react",
    "redux",
    "flux"
  ],
  "author": "gang",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-url-loader": "^1.0.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tslint": "^5.4.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-del-plugin": "^0.0.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@types/react": "^15.0.30",
    "@types/react-dom": "^15.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
